@model IEnumerable<LibraryOnlineSystem.Models.BookReserve>

@{
    ViewBag.Title = "ListOfReservations";
    Layout = "~/Views/Shared/_User_Layout.cshtml";
}

<h2>ListOfReservations</h2>


<table class="table">
    <tr>
        <th> Book Title</th>
        <th>
            @Html.DisplayNameFor(model => model.BookReserveId)
        </th>
      
        <th>
            @Html.DisplayNameFor(model => model.ReservationRequestTime)
        </th>
        <th>
            Book Serial Number
        </th>
       
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @ViewBag.TitleBook
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BookReserveId)
            </td>
           
            <td>
                @Html.ValueFor(modelItem => item.ReservationRequestTime, "{0:dd/MM/yyyy}")
            </td>
            <td>
                @ViewBag.BookSerialNumber
            </td>
         
           
        </tr>
    }

</table>